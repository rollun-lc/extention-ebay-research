{
  "name": "Ebay research",
  "version": "1.0.2",
  "manifest_version": 3,
  "author": "rollun.com",
  "action": {
    "default_title": "Ebay - research lots faster",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ebay.com/sh/research?*"
      ],
      "js": [
        "react.development.js",
        "axios.js",
        "utils.js",
        "react-dom.development.js",
        "script.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ]
}