{
  "name": "Ebay research",
  "version": "0.0.21",
  "manifest_version": 2,
  "author": "rollun.com",
  "background": {
    "scripts": [
      "hot-reload.js"
    ]
  },
  "browser_action": {
    "default_title": "Ebay - research lots faster",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ebay.com/sh/research?*"
      ],
      "js": [
        "react.development.js",
        "axios.js",
        "utils.js",
        "react-dom.development.js",
        "script.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}